    "format": "yarn run __internal__prettier -- --write",
    "format:check": "yarn run __internal__prettier -- --list-different",
    "__internal__prettier": "prettier '{src,uitest,unittest}/**/*{.ts,.tsx}'",
    "start": "oci-plugin-cli run start -c dev.config.js",
    "start:integration": "NON_PROD_ENDPOINT='https://datascience-int.us-ashburn-1.oci.oc-test.com/20190101' OVERRIDE_TELEMETRY_NAMESPACE='oci_datascience_integration' OVERRIDE_JOBRUN_TELEMETRY_NAMESPACE='oci_datascience_jobrun_integration' OVERRIDE_MD_TELEMETRY_NAMESPACE='oci_datascience_pre_modeldeploy' OVERRIDE_PIPELINE_RUN_TELEMETRY_NAMESPACE='oci_datascience_pipelinerun_integration' yarn run start",
    "start:preprod": "NON_PROD_ENDPOINT='https://datasciencepreprodproxying.us-ashburn-1.oci.oraclecloud.com/20190101' OVERRIDE_TELEMETRY_NAMESPACE='oci_datascience_preprod' OVERRIDE_JOBRUN_TELEMETRY_NAMESPACE='oci_datascience_jobrun_preprod' OVERRIDE_MD_TELEMETRY_NAMESPACE='oci_datascience_pre_modeldeploy' OVERRIDE_PIPELINE_RUN_TELEMETRY_NAMESPACE='oci_datascience_pipelinerun_preprod' yarn run start",
    "start:preprod-phx": "NON_PROD_ENDPOINT='https://pre.datascience.us-phoenix-1.oci.oraclecloud.com/20190101' OVERRIDE_TELEMETRY_NAMESPACE='oci_datascience_preprod' OVERRIDE_JOBRUN_TELEMETRY_NAMESPACE='oci_datascience_jobrun_preprod' OVERRIDE_MD_TELEMETRY_NAMESPACE='oci_datascience_pre_modeldeploy' OVERRIDE_PIPELINE_RUN_TELEMETRY_NAMESPACE='oci_datascience_pipelinerun_preprod' yarn run start",
    "start:preprod-oc2": "NON_PROD_ENDPOINT='https://datascience-pre.us-langley-1.oci.oraclegovcloud.com/20190101' OVERRIDE_TELEMETRY_NAMESPACE='oci_datascience_preprod' OVERRIDE_JOBRUN_TELEMETRY_NAMESPACE='oci_datascience_jobrun_preprod' OVERRIDE_MD_TELEMETRY_NAMESPACE='oci_datascience_pre_modeldeploy' OVERRIDE_PIPELINE_RUN_TELEMETRY_NAMESPACE='oci_datascience_pipelinerun_preprod' yarn run start",
    "build": "oci-plugin-cli run build -c prod.config.js",
    "build-development": "oci-plugin-cli run build -c uitest/webpack.test.config.js",
    "lint": "tslint -c \"tslint.json\" -e \"node_modules/**/*\" --project \"tsconfig.json\" \"./src/**/*.ts*\"",
    "open": "oci-plugin-cli run open -c dev.config.js",
    "test": "echo 'tests are disabled'",
    "test-watch": "oci-plugin-cli run test -e '--watch'",
    "test-coverage": "oci-plugin-cli run test -e '--coverage'",
    "test-update": "oci-plugin-cli run test -e '--updateSnapshot'",
    "uitest": "oci-plugin-cli run uitest",
    "translate": "loom-i18n-cli",
    "prready": "yarn format:check && yarn lint && yarn build && yarn test",
    "version": "echo $npm_package_version",
    "build-artifacts-api-client": "savant-client-generator maven -n artifacts-api-client -r ocir-release-maven-local -g com.oracle.pic.artifacts -a artifacts-api-spec -i 0.2.10 -p internal/artifacts-api-spec.yaml > /dev/null 2>&1"
  },
  "prettier": {
    "parser": "typescript",
    "tabWidth": 2,
    "printWidth": 100,
    "semi": true,
    "singleQuote": false
