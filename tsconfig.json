{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@codegen/*": ["../codegen/*"],
      "@clients/*": ["../clients/*"],
      "@unittest/*": ["../unittest/*"],
      "ReactTestUtils": ["./typings/*"]
    },
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es2015", "es2016", "es5", "es2019"],
    "jsx": "react",
    "target": "es5",
    "downlevelIteration": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true
  }
}
